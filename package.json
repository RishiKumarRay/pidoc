{
  "name": "piral-docs-tools",
  "version": "0.11.3",
  "description": "Tools for generating a dynamic documentation experience like for docs.piral.io.",
  "app": "./src/index.pug",
  "main": "./src/tools/index.js",
  "typings": "./src/scripts/types.ts",
  "scripts": {
    "build": "echo \"Error: no build specified\"",
    "lint": "echo \"Error: no lint specified\"",
    "test": "echo \"Error: no test specified\""
  },
  "bin": {
    "piral-docs": "./bin/cli.js"
  },
  "engines": {
    "node": ">=10.0"
  },
  "keywords": [
    "piral",
    "page",
    "documentation",
    "docs",
    "website",
    "modular",
    "smapiot"
  ],
  "pilets": {
    "externals": [
      "piral-docs-tools/components"
    ],
    "files": [
      {
        "from": "./src/scaffold/index.tsx",
        "to": "./src/index.tsx",
        "template": true
      }
    ]
  },
  "author": "smapiot",
  "homepage": "https://docs.piral.io",
  "license": "MIT",
  "dependencies": {
    "flexsearch": "^0.6.32",
    "keyword-extractor": "0.0.20",
    "markdown-it": "^12.0.6",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-div": "^1.1.0",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-front-matter": "^0.2.3",
    "markdown-it-highlightjs": "^3.5.0",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-replace-link": "^1.1.0",
    "markdown-it-smartarrows": "^1.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-video": "^0.6.3",
    "pug": "^3.0.2",
    "stickyfilljs": "^2.1.0",
    "yaml": "^1.10.2"
  },
  "peerDependencies": {
    "piral-core": "*",
    "piral-cli": "*",
    "piral-cli-parcel": "*",
    "react": "*",
    "react-dom": "*",
    "react-router-dom": "*"
  },
  "devDependencies": {
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "piral-core": "next",
    "piral-cli": "next",
    "piral-cli-parcel": "next",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.3.4"
  }
}
